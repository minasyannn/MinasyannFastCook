<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <!-- Set the background color of the rectangle -->
    <solid android:color="@null" />

    <!-- Set the corner radius of the rectangle -->
    <corners android:radius="13.5dp" />

    <!-- Set the border color and width of the rectangle -->
    <stroke
        android:width="2dp"
        android:color="#FB7813" />

</shape>

